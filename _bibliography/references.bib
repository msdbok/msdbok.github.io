@article{brown_risky_2024,
	title = {Risky Business: Navigating the World of Software Productivity},
	abstract = {Size and productivity are commonly cited as the two major software development cost drivers. Logic dictates that the two are related and inversely correlated. But what is the probabilistic range of uncertainty for productivity, given a software size? What is meant by “an 80\% confidence level for productivity”? Cost analysts often quantify uncertainty with an S-Curve; why can’t this be done for productivity directly? We use International Software Benchmarking Standards Group ({ISBSG}) data to estimate the distribution of productivity directly and provide closed-form formulas for the fitted distribution(s). We find that productivity (and, with certain assumptions, cost) can be estimated with an {SCurve} directly, using built-in Excel formulas, with no need for Monte Carlo simulation. This result has significant implications for almost any software development cost estimate and is particularly relevant to agile development efforts where time-boxed effort is generally fixed.},
	journaltitle = {Professional Development},
	author = {Brown, Dave and Cincotta, Kevin},
	date = {2024},
}
