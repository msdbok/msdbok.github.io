@article{amabile_satera_2003,
	title = {Satera Team at Imatron Systems, Inc.(A), The},
	abstract = {Escalating conflict has erupted within the Satera product development team, resulting from the conflicting cognitive styles of the two senior mechanical engineers. The conflict has taken a toll on both project progress and team morale, endangering one of the most important initiatives at Imatron Systems, Inc. After discussing the situation with {VP} of R\&D Rick Levinger, team leader Gary Pinto realizes he must take decisive action. This case presents a profile of the company, the team, the Satera project, and the team members, focusing on Pinto and the dueling engineers. Through a detailed description of their vastly different problem-solving preferences, and the interactions in which those differences become most apparent, the conflict presents a common problem that managers must face when working with creative people on creative projects.},
	author = {Amabile, Teresa M and Schatzel, Elizabeth},
	date = {2003},
}

